issn = {1939-1285},
journal = {Journal of Experimental Psychology: Learning, Memory, and Cognition},
keywords = {evidence that relatedness between,false memory,guessing,items in a,mem-,or semantic relations between,psychology,recognition,semantic priming,study items can facilitate,study set has been,testing effect,the role of associative,warning,well documented within cognitive},
number = {6},
pages = {1720--1730},
pmid = {22642236},
title = {{Interpolated task effects on direct and mediated false recognition: Effects of initial recall, recognition, and the ironic effect of guessing.}},
